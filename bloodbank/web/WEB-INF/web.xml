    <servlet-mapping>
        <servlet-name>UpdatedonorBE</servlet-name>
        <url-pattern>/UpdatedonorBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stockupdateBE</servlet-name>

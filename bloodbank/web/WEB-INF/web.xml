        <servlet-class>bloodbank.UpdatedonorBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stockupdateBE</servlet-name>
        <servlet-class>bloodbank.stockupdateBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchlocationBE</servlet-name>
        <servlet-class>bloodbank.searchlocationBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginpageBE</servlet-name>
        <servlet-class>bloodbank.loginpageBE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>stockviewBE</servlet-name>

    <servlet-mapping>
        <servlet-name>searchlocationBE</servlet-name>
        <url-pattern>/searchlocationBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginpageBE</servlet-name>
        <url-pattern>/loginpageBE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stockviewBE</servlet-name>
        <url-pattern>/stockviewBE</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
